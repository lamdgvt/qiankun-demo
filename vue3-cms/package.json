{
	"name": "pro-cms",
	"version": "0.0.0",
	"scripts": {
		"dev": "vite",
		"build:prod": "vue-tsc --noEmit --skipLibCheck && vite build",
		"build:test": "vue-tsc --noEmit --skipLibCheck && vite build --mode=testing",
		"build:stag": "vue-tsc --noEmit --skipLibCheck && vite build --mode=staging",
		"build:dev": "vue-tsc --noEmit && vite build --mode=development",
		"deploy:stag": "deploy-cli-service deploy --mode stag",
		"deploy:test": "deploy-cli-service deploy --mode test",
		"serve": "vite",
		"preview": "vite preview",
		"prettier": "prettier --write ."
	},
	"dependencies": {
		"ant-design-vue": "^3.2.13",
		"babel-eslint": "^10.1.0",
		"js-cookie": "^3.0.1",
		"pinia": "^2.0.17",
		"pinia-plugin-persistedstate": "^1.6.3",
		"vite-plugin-html": "^3.2.0",
		"vite-plugin-qiankun": "^1.0.15",
		"vue": "^3.2.25",
		"vue-router": "^4.0.12",
		"xss": "^1.0.13"
	},
	"devDependencies": {
		"@types/js-cookie": "^3.0.2",
		"@types/lodash": "^4.14.182",
		"@types/node": "^18.0.0",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"@vitejs/plugin-legacy": "^1.7.1",
		"@vitejs/plugin-vue": "^2.0.0",
		"@vitejs/plugin-vue-jsx": "^1.3.10",
		"compression-webpack-plugin": "^5.0.1",
		"deploy-cli-service": "^1.3.0",
		"eslint": "^8.6.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-vue": "^8.2.0",
		"less": "^4.1.3",
		"less-loader": "^11.0.0",
		"prettier": "2.5.1",
		"rollup-plugin-visualizer": "^5.6.0",
		"typescript": "^4.4.4",
		"vite": "^2.9.13",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-fonts": "^0.5.0",
		"vue-tsc": "^0.29.8"
	},
	"eslintConfig": {
		"extends": "airbnb-base",
		"parser": "babel-eslint"
	}
}
